{
  "expo": {
    "name": "Oniks EKS v2",
    "slug": "oniks-eks-app",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/eks_app_logo_v2.png",
    "scheme": "onikseks",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "extra": {
      "EXPO_BACKEND_URL": "https://oniksbilgi.com.tr/api",
      "router": {},
      "eas": {
        "projectId": "ff30de98-0030-4bd5-823a-6f363a7f412e"
      }
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/eks_app_logo_v2.png",
        "backgroundColor": "#E91E63"
      },
      "package": "com.oniksbilgi.eksapp",
      "edgeToEdgeEnabled": true
    },
    "androidStatusBar": {
      "backgroundColor": "#E91E63",
      "barStyle": "light-content"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/eks_app_logo_v2.png"
    },
    "plugins": [
      "expo-router",
      "expo-dev-client",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/eks_app_logo_v2.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#E91E63"
        }
      ],
      "expo-font",
      "expo-web-browser",
      "./plugins/withAndroidPackaging"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
